    "react-native-elements": "^1.1.0",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-render-html": "^4.1.2",
    "react-navigation": "^3.9.1",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.0.0",
    "jest": "^24.8.0",
    "react-test-renderer": "16.8.6"
